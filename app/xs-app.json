{
  "welcomeFile": "index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "/favicon.ico",
      "localDir": "resources"
    },
    {
      "source": "^/uploadCryptoData/(.*)$",
      "destination": "srv"
    },
    {
      "source": "^/downloadCryptoData/(.*)$",
      "destination": "srv"
    },
    {
      "source": "^/saas/(.*)$",
      "destination": "srv"
    },
    {
      "source": "^/srv/(.*)$",
      "destination": "srv"
    },
    {
      "source": "/(.*)",
      "destination": "srv"
    }
  ]
}
