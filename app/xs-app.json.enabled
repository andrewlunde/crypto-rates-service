{
  "welcomeFile": "index.html",
  "authenticationMethod": "none",
  "x_authenticationMethod": "route",
  "routes": [
    {
      "source": "^/srv/(.*)$",
      "x_authenticationType": "xsuaa",
      "x_scope": "$XSAPPNAME.User",
      "destination": "srv"
    },
    {
      "source": "/(.*)",
      "x_authenticationType": "none",
      "localDir": "resources"
    }
  ]
}
